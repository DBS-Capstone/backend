{
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "printWidth": 80,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "avoid",
